# 1.编辑好端口和对应的密码
# 2.添加或删除的用户都在 "port_password"{……} 中
# 3.用户信息格式，注意末尾的英文逗号："端口"："密码",  如 "8006": "123456",
# 4."method" 为加密方式，可修改，默认也行，例如aes-256-cfb
{
    "server":"0.0.0.0",
    "local_address":"127.0.0.1",
    "local_port":1080,
    "port_password":{
         "8989":"password0",
         "9001":"password1",
         "9002":"password2",
         "9003":"password3",
         "9004":"password4"
    },
    "timeout":300,
    "method":"aes-256-gcm",
    "fast_open": false
}
